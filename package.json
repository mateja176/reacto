{
  "name": "reacto",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "codegen": "graphql-codegen --config codegen.yml",
    "build": "tsc",
    "type-check": "tsc --noEmit",
    "format": "prettier src/**/*.ts --check",
    "format:fix": "prettier src/**/*.ts --write",
    "lint": "eslint src/**/*.ts",
    "ci": "cross-env CI=true concurrently --kill-others-on-fail --names format,lint,type-check,test --prefix-colors yellow,cyan,blue,green npm:format npm:lint npm:type-check npm:test",
    "start": "ts-node src/index.ts",
    "dev:server": "nodemon --watch src --ext ts --ignore 'src/**/*.spec.ts' --exec 'ts-node' src/index.ts",
    "dev": "concurrently --kill-others-on-fail --names server,merge-and-generate --prefix-colors cyan,blue npm:merge-and-generate:watch npm:dev:server",
    "serve": "node build/index.js",
    "seed": "ts-node scripts/seed.ts",
    "merge-schema": "ts-node scripts/mergeSchema.ts",
    "generate-ts": "ts-node scripts/generateTs.ts",
    "merge-and-generate": "npm run merge-schema && npm run generate-ts",
    "merge-and-generate:watch": "nodemon --watch src --ext graphql --ignore src/generated/schema.graphql --exec 'npm run merge-and-generate'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mateja176/reacto.git"
  },
  "keywords": [],
  "author": "Mateja Petrovic <mateja176@gmail.com> (https://github.com/mateja176)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mateja176/reacto/issues"
  },
  "homepage": "https://github.com/mateja176/reacto#readme",
  "devDependencies": {
    "@graphql-codegen/cli": "1.19.2",
    "@graphql-codegen/introspection": "1.18.0",
    "@graphql-codegen/typescript": "1.17.11",
    "@graphql-codegen/typescript-mongodb": "1.17.8",
    "@graphql-codegen/typescript-resolvers": "1.17.11",
    "@graphql-tools/merge": "^6.2.5",
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.9",
    "@types/express-jwt": "^0.0.42",
    "@types/fs-extra": "^9.0.4",
    "@types/jest": "^26.0.15",
    "@types/mongoose": "^5.10.2",
    "@types/node": "^14.14.9",
    "@types/nodemailer": "^6.4.0",
    "@types/nodemailer-mailgun-transport": "^1.4.2",
    "@types/prettier": "^2.1.5",
    "@types/ramda": "^0.27.32",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "commander": "^2.20.3",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "dotenv-cli": "^4.0.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.6",
    "prettier": "^2.2.0",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.1.2",
    "utility-types": "^3.10.0",
    "wait-on": "^5.2.0"
  },
  "dependencies": {
    "@typegoose/typegoose": "^7.4.5",
    "apollo-server-express": "^2.19.0",
    "bcrypt": "^5.0.0",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "graphql": "^15.4.0",
    "joi": "^17.3.0",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.6.3",
    "mongoose": "^5.10.18",
    "nodemailer": "^6.4.16",
    "nodemailer-mailgun-transport": "^2.0.2",
    "ramda": "^0.27.1",
    "reflect-metadata": "^0.1.13",
    "uuid": "^8.3.1"
  }
}
